							<script type="text/javascript">
     
$(document).ready(function() {

    $( '#rhbc417012' ).cycle({    
        fx: 'fade',
        paused: false,
        timeout: 4 * 1000,
        speed:  9 * 100,
        rev:    0, 
        continueAuto:true,
        swipe:true,
        log:false
    });

    $( '#rhbc417012' ).on( 'cycle-before', function( event, opts ) {
        $(".slide-show .prev").fadeIn("slow");
    });

    $( '#rhbc417012' ).on( 'cycle-after', function() {
        // shouldLoadNewContent();
    });

});

function shouldLoadNewContent(){
    var slideIndex = 0,
    totalSlides = 0,
    remainingToCurrentEnd = 0,
    fAdditionalSlidesAvailable = true,
    loadSize = 50;

    if(!fAdditionalSlidesAvailable){ return; };
 
    API = $( '#rhbc417012' ).data( 'cycle.API' );
    
    totalSlides = API.getSlideIndex();

    slideIndex = $('#rhbc417012').data("cycle.opts").currSlide + 1;

    if(totalSlides == slideIndex){
        fAdditionalSlidesAvailable = false;
        return;
    }

    remainingToCurrentEnd = totalSlides - slideIndex;
    
    if((remainingToCurrentEnd < loadSize) && ((remainingToCurrentEnd % loadSize) < 2)) {
        getSlideShowData("rhbc417012", totalSlides + 1 , loadSize);
    }
}

function getSlideShowData(promotionId, startPos, length){
    var slideShowDataUrl = '/sitewide/data/json/slide-show-data.jsp?promotionId=' + promotionId
            + '&start=' + startPos
            + '&length=' + length;
    $.ajax({
        type: "GET",
        url: slideShowDataUrl,
        dataType: "html",
        success: function (data) {
            $('#rhbc417012').cycle('add', $(data));
        }
    });
}
</script>
 
